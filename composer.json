{
  "name": "crothers/pocker",
  "description": "A PHP interface to the Docker restful API.",
  "type": "library",
  "version": "0.0.1-dev",
  "keywords": [
    "docker",
    "api",
    "containers"
  ],
  "homepage": "https://github.com/scrothers/pocker",
  "license": "MIT",
  "authors": [
    {
      "name": "Steven Crothers",
      "email": "steven.crothers@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/scrothers/pocker/issues",
    "wiki": "https://github.com/scrothers/pocker/wiki",
    "source": "https://github.com/scrothers/pocker"
  },
  "require": {
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "phpdocumentor/phpdocumentor": "~2.8"
  },
  "autoload": {
    "psr-4": {
      "Pocker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pocker\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
